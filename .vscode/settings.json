{
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "json.schemas": [
    {
      "fileMatch": ["*.docs.json"],
      "url": "./script/components-json/component.schema.json"
    },
    {
      "fileMatch": ["generated/components.json"],
      "url": "./script/components-json/output.schema.json"
    }
  ],
  "workbench.colorCustomizations": {
    "activityBar.background": "#01342C",
    "titleBar.activeBackground": "#02493E",
    "titleBar.activeForeground": "#E9FFFB"
  }
}
